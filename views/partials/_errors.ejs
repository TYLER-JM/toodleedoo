<% if(locals.error.length) { %>
  <div style="position: fixed; top:100px; right: 50px; opacity: .8;" class="alert alert-danger alert-dismissible fade show" role="alert">
      <h3><%- locals.error %></h3>
      <button type="button" class="close" data-dismiss="alert" aria-label="Close">
        <span aria-hidden="true">&times;</span>
      </button>
  </div>
<% } %>
